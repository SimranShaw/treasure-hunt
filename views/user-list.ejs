<!DOCTYPE html>
<html lang="en">
<head>
  <title>Dashboard</title>
  <link rel="stylesheet" href="./assets/user-list.css">
</head>
<body>
    <div class="header">
        <form method="post" action="/logOut" class="inline">
            <input type="hidden" name="extra_submit_param" value="extra_submit_value">
            <button class="logOut" type="submit" name="submit_param" value="Log Out" class="link-button">
              Log Out
            </button>
          </form>
    </div>
    <div class="table-data">
<h2 class="tbl">Player list and scores</h2>
    <table class="tbl" border="1">
        <tr>
            <th>INDEX</th>
            <th>MAIL ID</th>
            <th>LEVEL</th>
            <th>ACCURACY</th>
            <th>MISTAKES COMMITTED</th>
            <th>SCORE</th>

        </tr>
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.userMailID %></td>
            <td><%=data.levelUnlocked %></td>
            <td><%=data.accuracy %>%</td>
            <td><%=data.error %></td>
            <td><%=data.score %></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="6">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>